@model List<ResultServiceDto>









<section class="w3l-index3" id="about">
    <div class="midd-w3 pb-5">
        <div class="container pb-lg-5 pb-md-4 pb-2">
            <div class="row">
                <div class="col-lg-5 pr-lg-0">
                    <div class="w3l-left-img">
                    </div>
                </div>
                <div class="col-lg-7 pl-lg-0">
                    <div class="w3l-right-info">
                      
                            <h6 class="title-small">@ViewBag.Title</h6>

                       

                        
                        <h3 class="title-big">@ViewBag.SubTitle</h3>
                        <p class="mt-4">
                         @ViewBag.Desc1
                        </p>
                        <p class="mt-3 mb-4">
                            @ViewBag.Desc2       
                        </p>

                          <ul>
                            @foreach (var item in Model)
                            {

                                <li><span class="fa fa-check" aria-hidden="true">@item.ServiceName</span></li>
                           
                            }
                            </ul>
                            
                       
                      
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>