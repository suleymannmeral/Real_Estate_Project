@model RealEstate_Dapper_UI.Areas.AboutUs.Data.AboutUsDto.UpdateAboutUsWhyUsDto
@{
    ViewData["Title"] = "UpdateAboutUsWhyUs";
    Layout = "~/Views/Admin/Index.cshtml";
}
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6>Neden Biz Alanı Güncelleme Sayfası</h6>
                <form method="post">

                    <input type="hidden" asp-for="WhyUsId" />
                    <label> Başlık </label>
                    <input type="text" asp-for="Title" class="form-control" />
                    <label>Açıklama</label>
                    <br />
                    <input type="text" asp-for="Description" class="form-control" />
                    <label>Madde(1)</label>
                    <br />
                    <input type="text" asp-for="s1" class="form-control" />
                    <label>Madde(2)</label>
                    <br />
                    <input type="text" asp-for="s2" class="form-control" />
                    <label>Madde(3)</label>
                    <br />
                    <input type="text" asp-for="s3" class="form-control" />
                    <label>Madde(4)</label>
                    <br />
                    <input type="text" asp-for="s4" class="form-control" />
                    <br />

                    <button class="btn btn-primary">Kaydet</button>

                </form>
            </div>
        </div>
    </div>
</div>




